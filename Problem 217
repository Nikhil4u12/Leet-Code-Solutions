class Solution {
    public boolean containsDuplicate(int[] arr) {
         HashMap<Integer, Integer> map = new HashMap<>();
         int n = arr.length;
         for(int i =0; i<n;i++){
            if(map.containsKey(arr[i])) return true;
            else map.put(arr[i],i);
         }
         return false;
    }
   
}
